(defmacro if (condition then else)
    `(cond
        (,condition ,then)
        (T ,else))
)
(defmacro when (condition action)
    `(if ,condition ,action nil)
)
(defmacro unless (condition action)
    `(if ,condition nil ,action)
)

(defun assert-equals (actual expected)
    (unless (eq actual expected)
        (prints "assertation failed" "\nActual:" actual "\nExpected:" expected)))

(prints "                    <<< assert test >>> (2 == 2) (2 != 3)")
